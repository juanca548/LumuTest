{
  "name": "lumutest",
  "version": "2.0.0",
  "description": "Cucumber and Cypress Framework",
  "main": "index.js",
  "scripts": {
    "cy:testConsole": "npx cypress run --headless --browser chrome",
    "cy:testChrome": "npx cypress open --e2e --browser chrome"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanca548/LumuTest.git"
  },
  "keywords": [
    "cypress"
  ],
  "author": "Juan Jaramillo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/juanca548/LumuTest/issues"
  },
  "homepage": "https://github.com/juanca548/LumuTest#readme",
  "devDependencies": {
    "cypress": "^13.6.4",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "multiple-cucumber-html-reporter": "^3.6.2"
  },
  "cypress-cucumber-preprocessor": {
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    },
    "nonGlobalStepDefinitions": false
  }
}
